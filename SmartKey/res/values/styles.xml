<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppBaseTheme" parent="@android:style/Theme.Light" />
    <style name="AppTheme" parent="@style/AppBaseTheme" />
    <style name="SplashTheme" parent="@android:style/Theme.Black.NoTitleBar">
        <item name="android:windowBackground">@android:color/black</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:windowNoDisplay">false</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowDisablePreview">false</item>
        <item name="android:windowShowWallpaper">false</item>
    </style>
    <style name="bbk_title_bar_style" parent="@vivo:style/Theme.Vigour.Light.NoTitleBar">
        <item name="vivo:titleButtonStyle">@vivo:style/Vigour.TitleButton.Internet.Light</item>
        <item name="vivo:titleBtnIconStyle">@vivo:style/Vigour.TitleBtnIcon.Internet.Light</item>
    </style>
</resources>
